{
	"name": "@cssints/types",
	"version": "0.1.0",
	"description": "CSS type definitions for cssints compiler, including CSS functions from MDN BCD",
	"files": [
		"dist",
		"src"
	],
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "echo 'No build needed for types package'",
		"typecheck": "tsc --noEmit",
		"generate": "bun run scripts/generate-types.ts",
		"format": "oxfmt --write",
		"lint": "oxlint --type-aware"
	},
	"dependencies": {
		"@mdn/browser-compat-data": "latest",
		"csstype": "^3.1.3"
	},
	"devDependencies": {
		"typescript": "catalog:"
	}
}
