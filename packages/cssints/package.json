{
	"name": "cssints",
	"version": "1.0.0",
	"description": "CSS-in-TypeScript utilities with type-safe CSS functions and properties",
	"keywords": [
		"css",
		"css-in-ts",
		"types",
		"typescript"
	],
	"license": "ISC",
	"author": "",
	"files": [
		"lib",
		"scripts"
	],
	"type": "module",
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"types": "./lib/index.d.ts",
			"import": "./lib/index.js"
		},
		"./types": {
			"types": "./lib/types.gen.d.ts",
			"import": "./lib/types.gen.js"
		},
		"./syntax": {
			"types": "./lib/syntax.gen.d.ts",
			"import": "./lib/syntax.gen.js"
		},
		"./compat": {
			"types": "./lib/compat.gen.d.ts",
			"import": "./lib/compat.gen.js"
		},
		"./properties": {
			"types": "./lib/properties.gen.d.ts",
			"import": "./lib/properties.gen.js"
		},
		"./functions": {
			"types": "./lib/functions.gen.d.ts",
			"import": "./lib/functions.gen.js"
		}
	},
	"scripts": {
		"generate": "bun run scripts/generate-all.ts",
		"generate:force": "bun run scripts/generate-all.ts --force",
		"test": "vitest",
		"typecheck": "tsgo --noEmit",
		"lint": "oxlint --type-aware",
		"lint:fix": "oxlint --type-aware --fix"
	},
	"dependencies": {
		"@mdn/browser-compat-data": "^7.3.2",
		"@types/web": "^0.0.335",
		"@types/webidl2": "^24.4.3",
		"@webref/css": "^8.2.5",
		"@webref/idl": "^3.72.2",
		"bcd-idl-mapper": "^3.0.1",
		"csstype": "^3.2.3",
		"effect": "4.0.0-beta.5",
		"es-toolkit": "^1.44.0",
		"mdn-data": "^2.27.1",
		"webidl2": "^24.5.0"
	},
	"devDependencies": {
		"browserslist": "^4.28.1",
		"vitest": "^4.0.18"
	}
}
